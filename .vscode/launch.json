  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "LLDB fft1",
      "program": "/usr/bin/opt",
      "args": [
        "-load-pass-plugin",
        "${workspaceFolder}/build/lib/libCacheAnalysisPass.so",
        "-passes=lru-misses",
        "${workspaceFolder}/test/fft1.ll",
        "-o",
        "/dev/null"
      ],
      "cwd": "${workspaceFolder}"
    },
